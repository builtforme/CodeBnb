{
  "version": "2.3.4",
  "config": {
    "s3bucket": "your-s3-bucket"
  },
  "scripts": {
    "test": "jest --coverage",
    "zip": "zip -r lambda.zip *.js package.json node_modules html",
    "deploy": "npm version patch && $npm_package_scripts_zip && aws s3 cp lambda.zip s3://$npm_package_config_s3bucket/v$npm_package_version/lambda.zip && terraform apply -var=\"app_version=$npm_package_version\" -var=\"s3bucket=$npm_package_config_s3bucket\""
  },
  "dependencies": {
    "async": "^2.6.0",
    "aws-sdk": "^2.119.0",
    "bluebird": "^3.5.0",
    "dotenv": "^4.0.0",
    "github": "^12.0.0",
    "google-spreadsheet": "^2.0.4",
    "handlebars": "^4.0.11",
    "lambda-git": "^0.1.1",
    "moment": "^2.21.0",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "codecov": "^2.3.0",
    "jest": "^21.2.1",
    "sinon": "^4.0.0"
  }
}
